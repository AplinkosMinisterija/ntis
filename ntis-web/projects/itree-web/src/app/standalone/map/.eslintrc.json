{
  "extends": [
    "../../../../../../.eslintrc.json"
  ],
  "overrides": [
    {  
      "files": ["*.ts"],
      "rules": {
      "@angular-eslint/component-selector": [
        "error",
        {
          "prefix": "map",
          "style": "kebab-case",
          "type": "element"
        }
      ],
      "@angular-eslint/directive-selector": [
        "error",
        {
          "prefix": "map",
          "style": "camelCase",
          "type": "attribute"
        }
      ]
    }
  }
]
}