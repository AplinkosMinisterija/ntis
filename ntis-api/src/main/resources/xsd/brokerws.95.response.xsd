<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        elementFormDefault="qualified">

    <!--
    /**
     * VĮ Registrų centro NTR išrašo XSD failas
     * @version V.2303-14
     */
    -->

    <xs:element name="REGISTRAS">
        <xs:annotation><xs:documentation>NT registro numeris ir kiti duomenys [ntr311.registrai.reg_tarn_nr||'/'||ntr311.registrai.reg_nr]</xs:documentation></xs:annotation>
        <xs:complexType mixed="true">
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="ATRIBUTAI"  minOccurs="0" maxOccurs="1"/>
                <xs:element ref="ADRESAI"    minOccurs="0" maxOccurs="1"/>
                <xs:element ref="ASMENYS"    minOccurs="0" maxOccurs="1"/>
                <xs:element ref="DOKUMENTAI" minOccurs="0" maxOccurs="1"/>
                <xs:element ref="OBJEKTAI"   minOccurs="0" maxOccurs="1"/>
                <xs:element ref="SZNS_TERITORIJOS"   minOccurs="0" maxOccurs="1"/>
                <xs:element ref="TEISES"     minOccurs="0" maxOccurs="1"/>
                <xs:element ref="PAZYMOS"    minOccurs="0" maxOccurs="1"/>

                <xs:element name="FROM_XML_TABLE" type="xs:string" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>techninis duomuo. NENAUDOTINAS!</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element name="BUKLE_JSP"     type="xs:string" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>techninis duomuo. NENAUDOTINAS!</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element name="TV_DATA_TIKRA"  type="xs:string" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>techninis duomuo. NENAUDOTINAS!</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element name="IST"            type="xs:string" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>techninis duomuo. NENAUDOTINAS!</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element ref="SISTEMOS_INFORMACIJA" minOccurs="0" maxOccurs="1" />
                <xs:element ref="SYS_INFO"             minOccurs="0" maxOccurs="1" />
                <xs:element ref="SYS_INFO_XML"         minOccurs="0" maxOccurs="1" />

                <xs:element ref="INFO_SUM"   minOccurs="0" maxOccurs="1"/>

                <xs:element ref="KLAIDA"               minOccurs="0" maxOccurs="1" />


            </xs:choice>
            <xs:attribute name="tipas"         type="xs:string" use="optional">
                <xs:annotation><xs:documentation>išrašo formavimo nuostatos</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="su_istorija"   type="xs:long"   use="required">
                <xs:annotation><xs:documentation>požymis, ar išrašas suformuotas su istoriniais duomenimis (0=ne, 1=taip)</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="schema"        type="xs:string" use="optional">
                <xs:annotation><xs:documentation>konstanta "NTR311"</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="reg_id"        type="xs:long"   use="optional">
                <xs:annotation><xs:documentation>registro id [ntr311.registrai.reg_id]. NEBENAUDOTINAS!</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="anuliuotas"        type="xs:long"   use="optional">
                <xs:annotation><xs:documentation>anuliavimo pozymis</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="dok_tipas"     type="xs:long"   use="required">
                <xs:annotation><xs:documentation>dokumento tipo nuostata</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="su_asm_kodais" type="xs:long"   use="required">
                <xs:annotation><xs:documentation>požymis, ar išrašas suformuotas su asm. kodais (0=ne, 1=taip)</xs:documentation></xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>



    <!-- ************************************************************************************** -->
    <!-- ************************************************************************************** -->
    <!-- ************************************************************************************** -->
    <xs:element name="ATRIBUTAI">
        <xs:annotation><xs:documentation>registro atributai</xs:documentation></xs:annotation>
        <xs:complexType>
            <xs:choice maxOccurs="unbounded">

                <xs:element ref="XSD_VERSIJA" minOccurs="0" maxOccurs="1"/>

                <xs:element name="REG_ID" type="xs:long" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>registro id [ntr311.registrai.reg_id]</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element name="REG_TARN_NR" type="xs:long" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>registravimo tarnybos numeris [ntr311.registrai.reg_tarn_nr]</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element name="REG_NR" type="xs:long" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>registro numeris tarnyboje [ntr311.registrai.reg_nr]</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element ref="TIPAS"/>

                <xs:element name="SUDARYMO_DATA" type="xs:date" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>Registro sudarymo data</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element name="VERSIJA" type="xs:int" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>Registro versijos nr. [ntr311.registrai.reg_versija]</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element name="VERSIJOS_DATA" type="xs:date" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>Registro versijos data [ntr311.registrai.reg_versijos_data]</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element ref="STATUSAS"/>
                <xs:element ref="TVARKYTOJAS"/>

                <xs:element name="PASTABOS" type="xs:string">
                    <xs:annotation><xs:documentation>registro pastabos [ntr311.registrai.reg_pastabos]</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element ref="ANUL_TIPAS" minOccurs="0" maxOccurs="1"/>

                <xs:element ref="BUKLE"/>

                <xs:element name="ADRESAS" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>registro adresas (tekstinis) [ntr3_web.ntr_util.adresas_reg_obj(...)]</xs:documentation></xs:annotation>
                    <xs:complexType mixed="true">
                        <xs:attribute name="pastaba" type="xs:string" use="optional">
                            <xs:annotation><xs:documentation>adreso pastaba (jei yra)</xs:documentation></xs:annotation>
                        </xs:attribute>
                    </xs:complexType>
                </xs:element>

                <xs:element name="TERITORIJA" type="xs:string" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>Teritorija, kurioje yra registras (tekstu) [ntr3_web.ntr_util.adresas_reg_obj(...)]</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element ref="IDENTIFIKATORIUS" minOccurs="0" maxOccurs="unbounded"/>
            </xs:choice>
        </xs:complexType>
    </xs:element>

    <xs:element name="XSD_VERSIJA">
        <xs:annotation><xs:documentation>dokumento xsd versijos duomenys</xs:documentation></xs:annotation>
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="data" type="xs:date" use="required">
                        <xs:annotation><xs:documentation>versijos data</xs:documentation></xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>


    <xs:element name="TIPAS">
        <xs:annotation><xs:documentation>Registro tipo pavadinimas ir kiti duomenys</xs:documentation></xs:annotation>
        <!--
            <xs:complexType>
                <xs:simpleContent>
                    <xs:extension base="xs:string">
                        <xs:attribute name="tipas" type="xs:byte" use="required">
                        <xs:annotation><xs:documentation>registro tipo id [ntr311.registrai.regt_tipas]</xs:documentation></xs:annotation>
                        </xs:attribute>
                    </xs:extension>
                </xs:simpleContent>
            </xs:complexType>
        -->

        <xs:complexType mixed="true">
            <xs:choice maxOccurs="unbounded">
                <xs:element name="REIKSME" type="xs:string"  minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>pavadinimas</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element name="REIKSME_EN" type="xs:string"  minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>pavadinimas anglų k.</xs:documentation></xs:annotation>
                </xs:element>
            </xs:choice>

            <xs:attribute name="tipas"      type="xs:byte"    use="required">
                <xs:annotation><xs:documentation>registro tipo id [ntr311.registrai.regt_tipas]</xs:documentation></xs:annotation>
            </xs:attribute>
        </xs:complexType>


    </xs:element>


    <xs:element name="STATUSAS">
        <xs:annotation><xs:documentation>Statuso pavadinimas ir kiti duomenys (Dėmesio! Gali buti ir @ REGISTRAS, ir @ OBJEKTAS, ir @NAUDOJIMO_BUDAS!)</xs:documentation></xs:annotation>

        <xs:complexType mixed="true">
            <xs:choice maxOccurs="unbounded">
                <xs:element name="REIKSME" type="xs:string"  minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>pavadinimas</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element name="REIKSME_EN" type="xs:string"  minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>pavadinimas anglų k.</xs:documentation></xs:annotation>
                </xs:element>
            </xs:choice>

            <xs:attribute name="statusas"      type="xs:byte"    use="optional">
                <xs:annotation><xs:documentation>statuso kodas</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="osta_statusas" type="xs:integer" use="optional">
                <xs:annotation><xs:documentation>statuso pavadinimas</xs:documentation></xs:annotation>
            </xs:attribute>

        </xs:complexType>
    </xs:element>


    <xs:element name="TVARKYTOJAS">
        <xs:annotation><xs:documentation>Padalinio - registro tvarkytojo - pavadinimas ir kiti duomenys</xs:documentation></xs:annotation>
        <xs:complexType mixed="true">

            <xs:choice maxOccurs="unbounded">
                <xs:element name="PAVADINIMAS" type="xs:string"  minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>pavadinimas</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element name="PAVADINIMAS_EN" type="xs:string"  minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>pavadinimas anglų k.</xs:documentation></xs:annotation>
                </xs:element>
            </xs:choice>

            <xs:attribute name="adresas" type="xs:string" use="required">
                <xs:annotation><xs:documentation>padalinio adresas (tekstinis)</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="tel" type="xs:string" use="required">
                <xs:annotation><xs:documentation>padalinio tel. nr. (tekstinis)</xs:documentation></xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>


    <xs:element name="ANUL_TIPAS">
        <xs:annotation><xs:documentation>Anuliavimo pozymis</xs:documentation></xs:annotation>
        <xs:complexType mixed="true">

            <xs:attribute name="tipas" type="xs:integer" use="optional">
                <xs:annotation><xs:documentation>anuliavimo tipas</xs:documentation></xs:annotation>
            </xs:attribute>

        </xs:complexType>
    </xs:element>


    <xs:element name="BUKLE">
        <xs:annotation><xs:documentation>Būklės pavadinimas ir kiti duomenys (Dėmesio! Gali buti ir @ REGISTRAS, ir @ OBJEKTAS!)</xs:documentation></xs:annotation>

        <xs:complexType mixed="true">
            <xs:choice maxOccurs="unbounded">
                <xs:element name="REIKSME" type="xs:string"  minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>pavadinimas</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element name="REIKSME_EN" type="xs:string"  minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>pavadinimas anglų k.</xs:documentation></xs:annotation>
                </xs:element>
            </xs:choice>

            <xs:attribute name="bukle"      type="xs:integer" use="optional">
                <xs:annotation><xs:documentation>registro būklės kodas</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="data_nuo"   type="xs:date"    use="optional">
                <xs:annotation><xs:documentation>būklės galiojimo data nuo</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="obuk_bukle" type="xs:integer" use="optional">
                <xs:annotation><xs:documentation>objekto būklės kodas</xs:documentation></xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>


    <xs:element name="IDENTIFIKATORIUS">
        <xs:annotation><xs:documentation>identifikatorius</xs:documentation></xs:annotation>
        <xs:complexType>
            <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="pav" type="xs:string" use="required">
                        <xs:annotation><xs:documentation>pavadinimas</xs:documentation></xs:annotation>
                    </xs:attribute>

                    <xs:attribute name="pav_en" type="xs:string" use="optional">
                        <xs:annotation><xs:documentation>pavadinimas anglų k.</xs:documentation></xs:annotation>
                    </xs:attribute>

                    <xs:attribute name="tipas" type="xs:byte" use="required">
                        <xs:annotation><xs:documentation>tipas</xs:documentation></xs:annotation>
                    </xs:attribute>
                </xs:extension>
            </xs:simpleContent>
        </xs:complexType>
    </xs:element>


    <!-- ************************************************************************************** -->
    <!-- ************************************************************************************** -->
    <!-- ************************************************************************************** -->
    <xs:element name="ADRESAI">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="ADRESAS" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>


    <xs:element name="ADRESAS">
        <xs:annotation><xs:documentation>adresas "adresų registro komponentėmis"</xs:documentation></xs:annotation>
        <xs:complexType mixed="false">
            <xs:choice minOccurs="0" maxOccurs="unbounded">

                <xs:element name="ADM_ID" type="xs:long" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>Administracinio vieneto (savivaldybės) id</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element name="GYV_ID" type="xs:long" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>Gyvenamosios vietovės (miesto/kaimo) id</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element name="GAT_ID">
                    <xs:simpleType>
                        <xs:union>
                            <xs:simpleType><xs:restriction base="xs:string"><xs:length value="0"/></xs:restriction></xs:simpleType>
                            <xs:simpleType><xs:restriction base="xs:long"/></xs:simpleType>
                        </xs:union>
                    </xs:simpleType>
                </xs:element>
                <!--
                        <xs:element name="GAT_ID" type="xs:long" minOccurs="0" maxOccurs="1">
                            <xs:annotation><xs:documentation>Gatvės id</xs:documentation></xs:annotation>
                        </xs:element>
                -->
                <xs:element name="NAMO_NR" type="xs:string" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>Namo numeris</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element name="KORPUSO_NR" type="xs:string" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>Korpuso numeris</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element name="BUTO_NR" type="xs:string" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>Buto numeris</xs:documentation></xs:annotation>
                </xs:element>

            </xs:choice>

            <xs:attribute name="adr_id"  type="xs:int" use="optional">
                <xs:annotation><xs:documentation>adreso id</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="aro_kodas"  type="xs:long" use="optional">
                <xs:annotation><xs:documentation>AR objekto kodas</xs:documentation></xs:annotation>
            </xs:attribute>

        </xs:complexType>
    </xs:element>



    <!-- ************************************************************************************** -->
    <!-- ************************************************************************************** -->
    <!-- ************************************************************************************** -->
    <xs:element name="ASMENYS">
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element ref="ASMUO"/>
            </xs:choice>
        </xs:complexType>
    </xs:element>

    <xs:element name="ASMUO">
        <xs:annotation><xs:documentation>asmens duomenys [fiziniam ntr311.fiziniu_asmenvardziai.fav_vardas||' '||ntr311.fiziniu_asmenvardziai.fav_pavarde arba juridiniam ntr311.juridiniu_pavadinimai.jav_pav]</xs:documentation></xs:annotation>

        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded"><!-- minOccurs="0" - "juodos klaidos" atvejams, kuomet asmuo NERASTAS! -->

                <xs:element ref="PAVARDE"     minOccurs="0" maxOccurs="1"/>
                <xs:element ref="VARDAS"      minOccurs="0" maxOccurs="1"/>
                <xs:element ref="PAVADINIMAS" minOccurs="0" maxOccurs="1"/>

                <xs:element ref="ASMENVARDIS" minOccurs="0" />

                <xs:element name="ADRESAS" type="xs:string" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>Asmens adresas (tekstinis) [ntr311.asmenys.asm_adresas]</xs:documentation></xs:annotation>
                </xs:element>

            </xs:choice>
            <xs:attribute name="asm_id" type="xs:int" use="required">
                <xs:annotation><xs:documentation>Asmens id</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="tipas" type="xs:byte">
                <xs:annotation><xs:documentation>Asmens tipas (1=fizinis, 2=juridinis)</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="datos_pav" type="xs:string">
                <xs:annotation><xs:documentation>datos lauko pavadinimo trumpinys</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="datos_pav_en" type="xs:string">
                <xs:annotation><xs:documentation>datos lauko pavadinimo trumpinys anglų k.</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="data" type="xs:string">
                <xs:annotation><xs:documentation>data (fiz. asmeniui - gimimo, jur. asmeniui - steigimo)</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="kodas" type="xs:long">
                <xs:annotation><xs:documentation>asmens kodas</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="ivede" type="xs:string">
                <xs:annotation><xs:documentation>įrašą įvedęs padalinys (tekstu)</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="buvo_pakeitimu" type="xs:boolean">
                <xs:annotation><xs:documentation>požymis ar buvo pakeitimų</xs:documentation></xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>

    <xs:element name="PAVARDE" type="xs:string">
        <xs:annotation><xs:documentation>Asmens pavardė [ntr311.fiziniu_asmenvardziai.fav_pavarde]</xs:documentation></xs:annotation>
    </xs:element>

    <xs:element name="VARDAS" type="xs:string">
        <xs:annotation><xs:documentation>Asmens vardas [ntr311.fiziniu_asmenvardziai.fav_vardas]</xs:documentation></xs:annotation>
    </xs:element>


    <xs:element name="PAVADINIMAS" type="xs:string">
        <xs:annotation><xs:documentation>Jur. asmens pavadinimas|objekto tipo pavadinimas (Dėmesio! Gali buti ir @ ASMUO, ir @ OBJEKTAS!)</xs:documentation></xs:annotation>
    </xs:element>


    <xs:element name="PAVADINIMAS_EN" type="xs:string">
        <xs:annotation><xs:documentation>Jur. asmens pavadinimas|objekto tipo pavadinimas anglų k.(Dėmesio! Gali buti ir @ ASMUO, ir @ OBJEKTAS!)</xs:documentation></xs:annotation>
    </xs:element>

    <xs:element name="ASMENVARDIS">
        <xs:annotation><xs:documentation>vardas, pavardė / pavadinimas ir kiti duomenys</xs:documentation></xs:annotation>
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element ref="PAVARDE" />
                <xs:element ref="VARDAS" />
                <xs:element ref="PAVADINIMAS" />
            </xs:choice>

            <xs:attribute name="data_nuo" type="xs:date">
                <xs:annotation><xs:documentation>asmenvardžio data nuo</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="data_iki" type="xs:string" use="optional">
                <xs:annotation><xs:documentation>asmenvardžio data iki</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="ivede" type="xs:string" use="required">
                <xs:annotation><xs:documentation>įrašą įvedęs padalinys (tekstu)</xs:documentation></xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>


    <!-- ************************************************************************************** -->
    <!-- ************************************************************************************** -->
    <!-- ************************************************************************************** -->
    <xs:element name="DOKUMENTAI">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="DOKUMENTAS" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>


    <xs:element name="DOKUMENTAS">
        <xs:annotation><xs:documentation>dokumento pavadinimas ir kiti duomenys</xs:documentation></xs:annotation>
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded">

                <xs:element name="PAVADINIMAS" type="xs:string" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>dokumento pavadinimas</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element name="PAVADINIMAS_EN" type="xs:string" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>dokumento pavadinimas anglų k.</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element name="DOK_PASTABOS" type="xs:string" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>dokumento pastabos [ntr311.dokumentai.dok_pastabos]</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element name="EDOK_ARCHYVAS" type="xs:string" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>elektronines dokumento versijos el. archyvo identifikatorius</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element name="EDOK_ID" type="xs:string" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>elektronines dokumento versijos identifikatorius archyve</xs:documentation></xs:annotation>
                </xs:element>

                <xs:element name="RENGEJAS" type="xs:string" minOccurs="0" maxOccurs="1">
                    <xs:annotation><xs:documentation>dokumento rengejas</xs:documentation></xs:annotation>
                </xs:element>

            </xs:choice>

            <xs:attribute name="dok_nr"   type="xs:string">
                <xs:annotation><xs:documentation>dokumento numeris</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="dok_data" type="xs:date">
                <xs:annotation><xs:documentation>dokumento data</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="dok_id"   type="xs:long" use="required">
                <xs:annotation><xs:documentation>dokumento id</xs:documentation></xs:annotation>
            </xs:attribute>

            <xs:attribute name="doku_tipas"   type="xs:long" use="optional">
                <xs:annotation><xs:documentation>dokumento tipas (SZNS dokumentams)</xs:documentation></xs:annotation>
            </xs:attribute>

